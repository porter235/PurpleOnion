@model IEnumerable<PurpleOnion.Models.Order>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Orders</h2>
<p>
    @Html.ActionLink("Add Order", "New")
</p>
<ul>
    @foreach (var order in Model)
    {
        <li>
            @User.Identity.Name (@order.PickUpTime)(@order.Items)
            @Html.ActionLink("Edit", "Edit", new { id = order.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id = order.Id })
        </li>
    }
</ul>
